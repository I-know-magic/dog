<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dhc:dogHomeConfiguration xmlns:dhc="http://elite.polito.it/dogHomeConfiguration">
<dhc:controllables>
<dhc:device domoticSystem="EnOcean" id="$name" class="RockerSwitch"
	gateway="$gateway">
	<dhc:description>$description</dhc:description>
	<dhc:isIn>$location</dhc:isIn>
	<dhc:param value="$address" name="address" type="network"/>
	<dhc:param value="$eep" name="eep" type="network"/>
	<dhc:param value="$uid" name="uid" type="network"/>
	<dhc:notificationFunctionality class="ButtonMNotificationFunctionality">
		<dhc:notifications>
			<dhc:notification id="PressedMNotification_button1_$name" class="PressedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="pressed" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B1"/>
			</dhc:notification>
			<dhc:notification id="PressedMNotification_button2_$name" class="PressedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="pressed" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B2"/>
			</dhc:notification>
			<dhc:notification id="PressedMNotification_button3_$name" class="PressedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="pressed" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B3"/>
			</dhc:notification>
			<dhc:notification id="PressedMNotification_button4_$name" class="PressedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="pressed" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B4"/>
			</dhc:notification>
			<dhc:notification id="ReleasedMNotification_button1_$name" class="ReleasedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="released" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B1"/>
			</dhc:notification>
			<dhc:notification id="ReleasedMNotification_button2_$name" class="ReleasedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="released" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B2"/>
			</dhc:notification>
			<dhc:notification id="ReleasedMNotification_button3_$name" class="ReleasedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="released" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B3"/>
			</dhc:notification>
			<dhc:notification id="ReleasedMNotification_button4_$name" class="ReleasedMNotification">
				<dhc:param name="nParams" value="1"/>
				<dhc:param value="released" name="notificationName" />
				<dhc:param type="String" value="buttonID" name="notificationParamName" />
				<dhc:param name="buttonID" value="B4"/>
			</dhc:notification>
		</dhc:notifications>
	</dhc:notificationFunctionality>
	<dhc:state class="MultipleOnOffState">
		<dhc:statevalues>
			<dhc:statevalue name="off" class="MultipleOffStateValue">
				<dhc:param name="buttonID" value="B1"/>
			</dhc:statevalue>
			<dhc:statevalue name="on" class="MultipleOnStateValue">
				<dhc:param name="buttonID" value="B1"/>
			</dhc:statevalue>
		</dhc:statevalues>
	</dhc:state>
	<dhc:state class="MultipleOnOffState">
		<dhc:statevalues>
			<dhc:statevalue name="off" class="MultipleOffStateValue">
				<dhc:param name="buttonID" value="B2"/>
			</dhc:statevalue>
			<dhc:statevalue name="on" class="MultipleOnStateValue">
				<dhc:param name="buttonID" value="B2"/>
			</dhc:statevalue>
		</dhc:statevalues>
	</dhc:state>
	<dhc:state class="MultipleOnOffState">
		<dhc:statevalues>
			<dhc:statevalue name="off" class="MultipleOffStateValue">
				<dhc:param name="buttonID" value="B3"/>
			</dhc:statevalue>
			<dhc:statevalue name="on" class="MultipleOnStateValue">
				<dhc:param name="buttonID" value="B3"/>
			</dhc:statevalue>
		</dhc:statevalues>
	</dhc:state>
	<dhc:state class="MultipleOnOffState">
		<dhc:statevalues>
			<dhc:statevalue name="off" class="MultipleOffStateValue">
				<dhc:param name="buttonID" value="B4"/>
			</dhc:statevalue>
			<dhc:statevalue name="on" class="MultipleOnStateValue">
				<dhc:param name="buttonID" value="B4"/>
			</dhc:statevalue>
		</dhc:statevalues>
	</dhc:state>
</dhc:device>
</dhc:controllables>
</dhc:dogHomeConfiguration>